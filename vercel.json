{
    "version": 2,
    "builds": [
      {
        "src": "src/app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/app.js"
      }
    ],
    "env": {
      "MONGODB_URI": "@mongodb_uri",
      "JWT_SECRET": "@jwt_secret",
      "SENDGRID_API_KEY": "@sendgrid_api_key",
      "ADMIN_EMAIL": "@admin_email",
      "SENDER_EMAIL": "@sender_email"
    }
  }
